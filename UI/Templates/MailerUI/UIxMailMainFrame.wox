  </var:if><var:if condition="singleWindowModeEnabled" const:negate="YES"><var:if condition="mailboxContentStyle">
    DIV#mailboxContent
     { height: <var:string value="mailboxContentStyle" />; }
  </var:if></var:if><var:if condition="singleWindowModeEnabled">
    DIV#mailboxContent
     { height: auto;
       bottom: 0px; }
